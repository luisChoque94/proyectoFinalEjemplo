# Example environment variables
# Copy this file to .env and fill in your actual values

# Zoom JWT Token for Zoom SDK
ZOOM_JWT_TOKEN=your_zoom_jwt_token_here

# Zoom OAuth Credentials (for user authentication)
ZOOM_CLIENT_ID=your_zoom_client_id_here
ZOOM_CLIENT_SECRET=your_zoom_client_secret_here

# Moodle API URL (include the base URL, e.g., https://your-moodle-site.com)
MOODLE_URL=https://your-moodle-site.com

# Institutional email domain (include @, e.g., @university.edu)
INSTITUTIONAL_EMAIL=@your-institution.edu